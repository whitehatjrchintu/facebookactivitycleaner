{
    "name": "Facebook activity log cleaner.",
    "version": "1.0",
    "description": "This chrome extentsion can clear you facebook activity log. It can unlike all posts,remove friends and clear unfollowed,searched,created posts,created comments etc. activity from activity log.",
    "manifest_version": 2,
	"author": "Sumit Singh",
	
	"icons": {
		"16": "images/icons/16.png",
		"19": "images/icons/24.png",
		"38": "images/icons/32.png",
		"64": "images/icons/48.png",
		"128": "images/icons/128.png"
	},
	
	"browser_action": {
		"default_icon": {
			"16": "images/icons/16.png",
			"48": "images/icons/48.png",
			"128": "images/icons/128.png"
		}
	},
	
    "background": {
        "scripts": [
			"background.js"
		]
    },
	
    "web_accessible_resources": [
		"content.js"
	],
	
    "permissions": [
        "webNavigation",
        "<all_urls>"
    ]
}